                        </div>
                    </div>

                    <div class="sidebar-form">
                        <div class="input-group">
                            <input name="ctl00$txtKeyword" type="text" id="ctl00_txtKeyword" class="form-control" placeholder="查找..." />
                            <span class="input-group-btn">
                                <a id="ctl00_lnbSearch" class="btn btn-flat" href="javascript:__doPostBack(&#39;ctl00$lnbSearch&#39;,&#39;&#39;)"><i class="fa fa-search"></i></a>
                            </span>
                        </div>
                    </div>

                    <div id="ctl00_LeftMenu">
                        <ul class="sidebar-menu">

                            @{
                                List<DapperMvc.Models.MenuModel> menuModelList = new CommonTool.CacheHelper().Get<List<DapperMvc.Models.MenuModel>>("webRootMenuList");
                                string activeStr = Request.Params["active"] == null ? "" : Request.Params["active"].ToString();
                                string treeViewActive = "";
                                foreach (DapperMvc.Models.MenuModel item in menuModelList)
                                {
                                    if (activeStr != "")
                                    {
                                        if (Convert.ToInt32(activeStr.Split(',')[0]) == item.pk_Menu)
                                        {
                                            treeViewActive = "active";
                                        }
                                        else
                                        {
                                            treeViewActive = "";
                                        }
                                    }

                                    if (item.Visible)
                                    {
                                        <li class="treeview @treeViewActive">
                                            <a href="../#?active=1"><i class="fa @(item.Icon)"></i><span>@item.Name_CHS</span> <i class="fa fa-angle-left pull-right"></i></a>
                                            <ul class="treeview-menu">
                                                @foreach (DapperMvc.Models.MenuModel sonItem in item.SonMenuList)
                                                {
                                                    if (sonItem.Visible)
                                                    {
                                                        if (activeStr == (sonItem.ParentID + "," + sonItem.pk_Menu))
                                                        {
                                                            <li class="active"><a href="@sonItem.Url?active=@(sonItem.ParentID),@(sonItem.pk_Menu)" target="_blank"><i class="fa @(sonItem.Icon)"></i>@sonItem.Name_CHS</a></li>
                                                        }
                                                        else
                                                        {
                                                            <li><a href="@sonItem.Url?active=@(sonItem.ParentID),@(sonItem.pk_Menu)" target="_blank"><i class="fa @(sonItem.Icon)"></i>@sonItem.Name_CHS</a></li>
                                                        }
                                                    }

                                                }

                                            </ul>
                                        </li>
                                    }

                                }
                            }

                        </ul>
                    </div>

                </section>
            </aside>

            <!-- 内容 -->
            <div>


                @RenderBody()

            </div>

